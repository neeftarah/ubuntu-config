o/sublime-text-2-meld-diff/master/packages.json": [{"description": "Add diff files in Meld option in side bar menu.", "readme": null, "donate": null, "name": "Meld diff", "issues": null, "previous_names": [], "homepage": "https://github.com/mablo/sublime-text-2-meld-diff", "author": "Michal Prochowski <michal@prochowski.pl>", "releases": [{"version": "1.0.1", "date": "2013-01-15 15:00:00", "sublime_text": "<3000", "platforms": ["linux"], "url": "https://codeload.github.com/mablo/sublime-text-2-meld-diff/zip/1.0.1"}], "buy": null, "labels": []}], "https://raw.github.com/tillig/SublimeMSBuild/master/packages.json": [{"description": "Sublime Text package for editing and executing MSBuild scripts.", "readme": "https://raw.github.com/tillig/SublimeMSBuild/master/README.md", "donate": "https://www.gittip.com/on/github/tillig/", "name": "MSBuild", "issues": "https://github.com/tillig/SublimeMSBuild/issues", "previous_names": [], "homepage": "https://github.com/tillig/SublimeMSBuild", "author": "tillig", "releases": [{"version": "1.2.0", "date": "2014-01-29 00:16:21", "sublime_text": "*", "platforms": ["*"], "url": "https://codeload.github.com/tillig/SublimeMSBuild/zip/1.2.0"}], "buy": null, "labels": ["language syntax", "auto-complete", "build system"]}], "https://raw.github.com/Andr3as/Sublime-SurroundWith/master/packages.json": [{"description": "A 'surround with' ability for Sublime Text like Eclipse has.", "readme": "https://raw.github.com/Andr3as/Sublime-SurroundWith/master/README.md", "donate": "https://www.gittip.com/on/github/Andr3as/", "name": "SurroundWith", "issues": "https://github.com/Andr3as/Sublime-SurroundWith/issues", "previous_names": [], "homepage": "http://andrano.de/SurroundWith/", "author": "Andr3as", "releases": [{"version": "2013.09.14.21.08.13", "date": "2013-09-14 21:08:13", "sublime_text": "*", "platforms": ["*"], "url": "https://codeload.github.com/Andr3as/Sublime-SurroundWith/zip/master"}], "buy": null, "labels": ["text manipulation"]}], "https: